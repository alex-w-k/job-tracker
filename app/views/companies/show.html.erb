<nav>
  <div class="nav-wrapper">
    <div class="col s11">
      <%= link_to 'Home', root_path, class: "breadcrumb"%>
      <%= link_to @company.name, company_path(@company), class: "breadcrumb" %>
    </div>
  </div>
</nav>
<ul class="collection with-header">
  <li class="collection-header"><h4>All Jobs for <strong><%= @company.name %></strong></h4></li>
  <% @company.jobs.each do |job| %>
    <li class="collection-item"><%= job.title %></li>
  <% end %>
</ul>
